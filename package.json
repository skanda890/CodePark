{
  "name": "codepark",
  "version": "2.0.0",
  "description": "Code playground with enhanced features",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security-check": "npm audit --production",
    "snyk-test": "snyk test --severity-threshold=high",
    "update:deps": "npm update",
    "github:check-limit": "node scripts/github-api-rate-limit-reset.js --check",
    "github:check-limit:json": "node scripts/github-api-rate-limit-reset.js --check --json",
    "github:monitor-limit": "node scripts/github-api-rate-limit-reset.js --monitor",
    "github:wait-reset": "node scripts/github-api-rate-limit-reset.js --wait",
    "github:reset-recommendations": "node scripts/github-api-rate-limit-reset.js --reset"
  },
  "dependencies": {
    "express": "5.0.0-beta.1",
    "mongoose": "^8.6.0-rc.0",
    "redis": "^4.7.0-alpha.1",
    "ioredis": "^5.4.0-rc.1",
    "axios": "^1.7.0-alpha.1",
    "speakeasy": "^2.0.0-beta.1",
    "qrcode": "^1.5.0-alpha.1",
    "socket.io": "^4.8.0-rc.1",
    "dotenv": "^16.4.0-beta.1",
    "cors": "^2.8.5-alpha.1",
    "helmet": "^7.1.0-rc.1",
    "express-rate-limit": "^7.3.0-beta.1",
    "rate-limit-redis": "^4.2.0-alpha.1",
    "express-validator": "^7.2.0-rc.1",
    "compression": "^1.7.4-beta.1",
    "uuid": "^9.0.0-alpha.1",
    "html-escape": "^1.0.3-rc.1",
    "pino": "^8.21.0-beta.1",
    "pino-pretty": "^10.3.0-alpha.1",
    "argon2": "^0.31.0-rc.1",
    "jsonwebtoken": "^9.1.0-beta.1",
    "bullmq": "^5.14.0-alpha.1",
    "express-async-errors": "^3.1.1-rc.1"
  },
  "devDependencies": {
    "jest": "^29.7.0-beta.1",
    "supertest": "^7.0.0-alpha.1",
    "nodemon": "^3.1.0-rc.1",
    "eslint": "^8.57.0-beta.1",
    "snyk": "^1.1300.0-alpha.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "code",
    "playground",
    "monitoring",
    "webhooks",
    "mfa"
  ],
  "author": "skanda890",
  "license": "MIT"
}
